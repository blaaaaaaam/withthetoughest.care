    <section class="text thistle">
      <h2 class="heading">Blessed Milk Thistle</h2>
      <div>{{ thistle | markdownify | safe }}</div>
    </section>
    <section class="text bio">
      <h2 class="heading">Biography</h2>
      <div>{{ bio | markdownify | safe }}</div>
    </section>
    <section class="text acknowledgements">
      <h2 class="heading">Acknowledgements</h2>
      <div>{{ acknowledgements | markdownify | safe }}</div>
    </section>
    <section class="text grant">
      <h2 class="heading"></h2>
      <div>{{ grant | markdownify | safe }}</div>
      <div class="logos">
        <img src="{{ '/assets/img/ausco_logo.png' }}" alt="Australia Council for the Arts logo" />
        <img src="{{ '/assets/img/parra_logo.png' }}" alt="City of Parramatta logo" />
        <img src="{{ '/assets/img/hc_logo.png' }}" alt="Hawai‘i Contemporary logo" />
      </div>
    </section>
    <section class="text credit">
      <p>Design & development by <a href="https://atanazefti.com" target="_blank">Alexander Tanazefti</a></p>
    </section>
  </main>
